{
  "name": "LivePraise",
  "version": "0.1.0",
  "main": "main.js",
  "author": {
    "name": "Thiago de Lucena Sobrinho",
    "email": "thiagolucenaelvis@gmail.com"
  },
  "contributors": {},
  "scripts": {
    "start": "electron main.js",
    "win32": "electron-packager . DWS --platform=win32 --arch=ia32 --icon=app/icon/cadimos.ico --prune=true --out=release-builds --asar --overwrite",
    "win64": "electron-packager . DWS --platform=win32 --arch=x64 --icon=app/icon/cadimos.ico --prune=true --out=release-builds --asar --overwrite",
    "linux32": "electron-packager . DWS --platform=linux --arch=ia32 --icon=app/icon/cadimos.ico --prune=true --out=release-builds --asar --overwrite",
    "linux64": "electron-packager . DWS --platform=linux --arch=x64 --icon=app/icon/cadimos.ico --prune=true --out=release-builds --asar --overwrite",
    "osx": "electron-packager . DWS --platform=darwin --arch=x64 --icon=app/icon/cadimos.ico --prune=true --out=release-builds --asar --overwrite",
    "build": "electron-packager . --all",
    "rebuild": "electron-rebuild -f -w sqlite3"
  },
  "dependencies": {
    "child-process-promise": "^2.2.1",
    "draggabilly": "^2.2.0",
    "electron-prebuilt": "^1.4.13",
    "express": "^4.16.3",
    "graceful-fs": "^4.1.15",
    "jquery": "^3.3.1",
    "node-pre-gyp": "^0.10.3",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "sqlite3": "^4.0.6"
  },
  "description": "Live Praise (Software de Projeção para Igrejas)",
  "devDependencies": {
    "debug": "^3.2.6",
    "electron": "^5.0.0-beta.2",
    "electron-packager": "^12.1.1",
    "electron-rebuild": "^1.8.2"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/cadimos/livepraise.git"
  },
  "keywords": [
    "Projeção",
    "DataShow",
    "Igreja"
  ],
  "bugs": {
    "url": "https://bitbucket.org/cadimos/livepraise/issues",
    "email": "livepraise@teraidc.com.br"
  },
  "license": "ISC",
  "homepage": "https://bitbucket.org/cadimos/livepraise#readme"
}
