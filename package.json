{
  "name": "dsw",
  "version": "0.1.0",
  "main": "main.js",
  "author": "Thiago Lucena",
  "scripts": {
    "start": "electron main.js",
    "win32": "electron-packager . DWS --platform=win32 --arch=ia32 --icon=app/icon/cadimos.ico --prune=true --out=release-builds --asar",
    "win64": "electron-packager . DWS --platform=win32 --arch=x64 --icon=app/icon/cadimos.ico --prune=true --out=release-builds",
    "linux32": "electron-packager . DWS --platform=linux --arch=ia32 --icon=app/icon/cadimos.ico --prune=true --out=release-builds --asar",
    "linux64": "electron-packager . DWS --platform=linux --arch=x64 --icon=app/icon/cadimos.ico --prune=true --out=release-builds",
    "osx": "electron-packager . DWS --platform=darwin --arch=x64 --icon=app/icon/cadimos.ico --prune=true --out=release-builds --asar",
    "build": "electron-packager . --all"
  },
  "dependencies": {
    "child-process-promise": "^2.2.1",
    "debug": "^3.1.0",
    "draggabilly": "^2.2.0",
    "electron-rebuild": "^1.8.2",
    "express": "^4.16.3",
    "jquery": "^3.3.1",
    "node-pre-gyp": "^0.10.3",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "socketio": "^1.0.0",
    "sqlite3": "^4.0.2"
  },
  "description": "DSW (Data Show Web)",
  "devDependencies": {
    "electron": "^4.0.0-beta.7",
    "electron-packager": "^12.1.1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/cadimos/dsw.git"
  },
  "keywords": [
    "Projeção",
    "DataShow",
    "Igreja"
  ],
  "license": "ISC",
  "homepage": "https://bitbucket.org/cadimos/dsw#readme"
}
