  },
  "scripts": {
    "start": "electron main.js",
    "win32": "electron-packager . LivePraise --platform=win32 --arch=ia32 --icon=app/icon/livepraise.ico --prune=true --out=release-builds --asar --overwrite",
    "win64": "electron-packager . LivePraise --platform=win32 --arch=x64 --icon=app/icon/livepraise.ico --prune=true --out=release-builds --asar --overwrite",
    "linux32": "electron-packager . LivePraise --platform=linux --arch=ia32 --icon=app/icon/livepraise.png --prune=true --out=release-builds --asar --overwrite",
    "linux64": "electron-packager . LivePraise --platform=linux --arch=x64 --icon=app/icon/livepraise.png --prune=true --out=release-builds --asar --overwrite",
    "osx": "electron-packager . LivePraise --platform=darwin --arch=x64 --icon=app/icon/livepraise.icns --prune=true --out=release-builds --asar --overwrite",
    "rebuild": "electron-rebuild -f -w sqlite3"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/cadimos/livepraise.git"
  },
  "private": true,
  "dependencies": {
    "blueimp-md5": "^2.10.0",
    "child-process-promise": "^2.2.1",
    "draggabilly": "^2.2.0",
    "express": "^4.17.0",
    "graceful-fs": "^4.1.15",
    "jquery": "^3.4.1",
    "jquery-confirm": "^3.3.2",
    "node-abi": "^2.8.0",
    "node-pre-gyp": "^0.13.0",
    "request": "^2.88.0",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "sqlite3": "^4.0.8",
    "systeminformation": "^4.5.1",
    "webcamjs": "^1.0.25"
  },
  "devDependencies": {
    "debug": "^4.1.1",
    "electron": "^5.0.3",
    "electron-packager": "^13.1.1",
    "electron-rebuild": "^1.8.5"
  }
}
